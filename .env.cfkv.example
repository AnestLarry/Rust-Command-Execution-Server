# Example .env file for Cloudflare KV configuration

# Database engine to use: sqlite or cloudflare_kv
DB_ENGINE=cloudflare_kv

# Cloudflare Account ID
CF_ACCOUNT_ID=your_cloudflare_account_id

# Cloudflare KV Namespace ID
CF_KV_NAMESPACE_ID=your_cloudflare_kv_namespace_id

# Cloudflare API Token with KV permissions
CF_API_TOKEN=your_cloudflare_api_token

# CORS origins (comma-separated)
CORS_ORIGINS=["*"]

# DB_URL is not used for cloudflare_kv, but keep it for consistency if needed by BaseSettings
# DB_URL=
