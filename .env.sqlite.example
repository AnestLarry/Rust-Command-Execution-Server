# Example .env file for SQLite configuration

# Database engine to use: sqlite or cloudflare_kv
DB_ENGINE=sqlite

# Database URL for SQLite
# For a local file: sqlite:///./rces.db
# For an in-memory database: sqlite:///:memory:
DB_URL=sqlite:///./rces.db

# CORS origins (comma-separated)
CORS_ORIGINS=["*"]
